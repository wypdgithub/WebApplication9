        function CheckingLogin(id) {
            if (id == 1) {
                jQuery("#Log_Submit").attr("disabled", true);
                jQuery("#Log_Submit").html("登录中");
                layer.load(1, {
                    shade: [0.1, '#fff'] //0.1透明度的白色背景
