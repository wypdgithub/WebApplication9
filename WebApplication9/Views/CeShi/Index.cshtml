        function LoginBtn() {
            var name = jQuery("#txtUserName").val();
            var pwd = jQuery("#txtUserPwd").val();
            var code = jQuery("#code").val();
            if (name == "") {
                layer.confirm('登录名不能为空', {
                    btn: ['确定'] //按钮
                });
                jQuery("#txtUserName").focus();

                return false;
            } else if (pwd == "") {
                layer.confirm('密码不能为空', {
                    btn: ['确定'] //按钮
                });
                jQuery("#txtUserPwd").focus();
                return false;
            } else if (code == "") {
                layer.confirm('验证码不能为空', {
                    btn: ['确定'] //按钮
                });
                jQuery("#code").focus();
                return false;
            } else {
                return true;
            }
        }
        function CheckUserDataValid() {
            if (!LoginBtn()) {
                return false;
            } else {
                CheckingLogin(1);
                var userName = jQuery("#txtUserName").val();
                var userPwd = jQuery("#txtUserPwd").val();
                var parm = '';
                var url = '';
                parm = 'action=login&user=' + escape(userName) + '|' + escape(userPwd);
                url = '/Frame/Frame.ashx';
                getAjax(url, parm, function (rs) {
                    if (parseInt(rs) == 1) {
                        jQuery("#txtCode").focus();
                        layer.confirm('验证码输入不正确', {
                            btn: ['确定'] //按钮
                        });

                        CheckingLogin(0);
                        ToggleCode("Verify_codeImag", '/Ajax/Verify_code.ashx');
                        return false;
                    } else if (parseInt(rs) == 2) {
                        layer.confirm('账户被锁,联系管理员', {
                            btn: ['确定'] //按钮
                        });
                        CheckingLogin(0);
                        return false;
                    } else if (parseInt(rs) == 4) {
                        layer.confirm('账户或密码有错误', {
                            btn: ['确定'] //按钮
                        });
                        CheckingLogin(0);
                        return false;
                    } else if (parseInt(rs) == 3) {
                        SetCookie("lgname", userName);
                        SetCookie("lgpwd", userPwd);
                        setTimeout(Load, 1000);
                    } else {
                        CheckingLogin(0);
                        layer.confirm('服务器连接不上,联系管理员！', {
                            btn: ['确定'] //按钮
                        });
                        window.location.href = window.location.href.replace('#', '');
                        return false;
                    }
                });
            }
        }

